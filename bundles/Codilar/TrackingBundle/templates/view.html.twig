{% extends '@CodilarTracking/default.html.twig' %}

{% block body %}
	{% block content %}
    <h1>Listing </h1>

	<div class="container text-center">
		<div class="box">
			<div class="box-body">
				<table class="table table-bordered">
					<thead class="bg-primary text-white">
						<tr>
							<th>User ID</th>
							<th>Login Time</th>
							<th>Logout Time</th>
							<th>Action Taken</th>
							<th>Action ID</th>
						</tr>
					</thead>
					<tbody>
						{% for track in list %}
						<tr>
							<td>{{ track.userId }}</td>
                            <td>{{ track.login|date('Y-m-d H:i:s') }}</td>
                            <td>{{ track.logout|date('Y-m-d H:i:s') }}</td>
							<td>{{ track.action }}</td>
							<td>{{ track.objectId }}</td>
						</tr>
						{% endfor %}
					</tbody>
				</table>
			</div>
		</div>
	</div>
	

		{# {% for vote in list %}
			<div class="vote">
				<p>Vote ID:
					{{ vote.id }}</p>
				<p>Vote score:
					{{ vote.score }}</p>
				<p>Vote username:
					{{ vote.username }}</p>
			</div>
		{% endfor %} #}

	{% endblock %}
{% endblock %}
